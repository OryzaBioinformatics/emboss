# Example ACD file for QA testing - for calculated attributes
#

appl: qatestcalcseq [ groups: "test" doc: "ACD file for QA testing" ]

sequence: testsequence [ param: Y ]

string: testlength [
	 standard: "Y"
	 info: "Length"
	 default: "$(testsequence.length)"
]

string: testname [
	 standard: "Y"
	 info: "Name"
	 default: "$(testsequence.name)"
]

string: testbegin [
	 standard: "Y"
	 info: "Begin"
	 default: "$(testsequence.begin)"
]

string: testend [
	 standard: "Y"
	 info: "End"
	 default: "$(testsequence.end)"
]

string: testprotein [
	 standard: "Y"
	 info: "Protein"
	 default: "$(testsequence.protein)"
]

string: testnucleic [
	 standard: "Y"
	 info: "Nucleic"
	 default: "$(testsequence.nucleic)"
]
