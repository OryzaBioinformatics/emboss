application: backtranseq [
  documentation: "Back translate a protein sequence"
  groups: "Nucleic:Translation, Protein:Composition"
]

section: input [
  information: "Input section"
  type: "page"
]

  sequence: sequence [
    parameter: "Y"
    type: "gapstopprotein"
  ]

  codon: cfile [
    additional: "Y"
    default: "Ehuman.cut"
  ]

endsection: input

section: output [
  information: "Output section"
  type: "page"
]

  seqout: outfile [
    parameter: "Y"
    type: "gapnucleotide"
  ]

endsection: output
