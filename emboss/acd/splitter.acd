appl: splitter [
  documentation: "Split a sequence into (overlapping) smaller sequences"
  groups: "Edit"
]

seqall: sequence  [
  parameter: "Y"
  type: "any"
]

seqoutall: outseq  [
  parameter: "Y"
]

integer: size  [
  optional: "Y"
  minimum: "1"
  default: "10000"
  prompt: "Size to split at"
]

integer: overlap  [
  optional: "Y"
  minimum: "0"
  default: "0"
  prompt: "Overlap between split sequences"
]
