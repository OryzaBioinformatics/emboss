# NTHSEQ application
# AJAX COMMAND DEFINITION (ACD) FILE

appl: nthseq [
	doc: "Writes one sequence from a multiple set of sequences"
	groups: "Edit"
]

  seqall: sequence [
	param: Y 
  ]

  int: number [ 
	req: Y
	min: 1
	def: 1
	prompt: "The number of the sequence to output"
  ]

  seqout: outseq [ 
	param: Y 
  ]
