appl: plotorf [
  documentation: "Plot potential open reading frames"
  groups: "Nucleic:Gene finding, Nucleic:Translation"
]

sequence: sequence  [
  parameter: "Y"
  type: "dna"
]

xygraph: graph  [
  required: "Y"
  multiple: "6"
]

string: start  [
  optional: "N"
  default: "ATG"
  information: "Start codons"
]

string: stop  [
  optional: "N"
  default: "TAA,TAG,TGA"
  information: "Stop codons"
]
