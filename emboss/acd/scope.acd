appl: scope [
        doc: "Convert raw scop classification file to embl-like format"
        groups: "Utils:Database creation"
]


infile: infile [
	prompt: "Name of scop file for input (raw format)"
	def: "scop.orig"
	param: Y
]

outfile: outfile [
	prompt: "Name of scop file for output (embl-like format)"
	def: "Escop.dat"
	param: Y
]
