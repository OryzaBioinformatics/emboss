appl: seqmatchall [
  documentation: "Does an all-against-all comparison of a set of sequences"
  groups: "Alignment:Local"
]

seqset: sequence  [
  parameter: "Y"
  type: "any"
]

integer: wordsize  [
  required: "Y"
  default: "4"
  minimum: "2"
  information: "Word size"
]

outfile: outfile  [
  parameter: "Y"
]
