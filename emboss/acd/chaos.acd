appl: chaos [
  documentation: "Create a chaos game representation plot for a sequence"
  groups: "Nucleic:Composition"
]

sequence: sequence  [
  parameter: "Y"
  type: "dna"
]

bool: data  [
  optional: "N"
  default: "N"
  information: "Display as data"
]

graph: graph  [
  required: "@(!$(data))"
]

outfile: outfile  [
  required: "$(data)"
  nullok: "Y"
]
