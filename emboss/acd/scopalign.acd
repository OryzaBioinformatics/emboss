appl: scopalign [
  documentation: "Generate alignments for SCOP families"
  groups: "Protein:3D Structure"
]

infile: scopf  [
  parameter: "Y"
  prompt: "Name of scop file for input (embl-like format)"
  default: "Escop.dat"
]

string: path  [
  parameter: "Y"
  prompt: "Location of alignment files for output"
  default: "./"
]
