application: ajbad [
  documentation: "Test file for ACD parsing"
  groups: "Test"
]

string: typea  [
  standard: "y"
  information: "Sequence type"
  valid: "valid value test"
]

sequence: sequence  [
  parameter: "y"
  type: "$(typea)"
]

string: typeb  [
  standard: "y"
  default: "$(seqa.type)"
  expected: "expected value"
  information: "Second type"
]

sequence: bsequence  [
  parameter: "y"
  type: "$(seqa.type)"
]

outfile: outfile  [
  parameter: "y"
  knowntype: "ajbad output"
]

codon: cod  [
  name: "eco"
]

string: date  [
  standard: "Y"
  default: "00/00/00"
  maxlength: "8"
  information: "Index date"
  pattern: "^([0-9]+.[0-9]+.[0-9]+)?$"
]

integer: a  [
  minimum: "0"
  information: "First number"
]

integer: b  [
  default: "15"
  information: "Second number"
]

float: c  [
  minimum: "-5"
  maximum: "6.6666"
  precision: "2"
  default: "1.6"
  information: "Third number"
]

list: frames  [
  standard: "Y"
  default: "1,2,3,4,5,6"
  minimum: "0"
  maximum: "6"
  header: "Select Frames To Translate"
  values: "0: None, 1: F1,2: F2,3: F3,4: R1,5: R2,6: R3"
  delimiter: ","
  codedelimiter: ":"
  information: "Select one or more values"
  button: "Y"
]
