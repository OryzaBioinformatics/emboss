appl: pscan [
  documentation: "Scans proteins using PRINTS"
  groups: "Protein:Motifs"
]

seqall: sequence  [
  parameter: "Y"
  type: "Protein"
]

integer: emin  [
  required: "Y"
  minimum: "1"
  maximum: "20"
  default: "2"
  information: "Minimum number of elements per fingerprint"
]

integer: emax  [
  required: "Y"
  minimum: "$(emin)"
  maximum: "20"
  default: "20"
  information: "Maximum number of elements per fingerprint"
]

outfile: outfile  [
  parameter: "Y"
]
