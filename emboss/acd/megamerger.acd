appl: megamerger [
  documentation: "Merge two large overlapping nucleic acid sequences"
  groups: "Alignment:Consensus"
]

sequence: seqa  [
  parameter: "Y"
  type: "DNA"
]

sequence: seqb  [
  parameter: "Y"
  type: "DNA"
]

seqout: outseq  [
  parameter: "Y"
  extension: "merged"
]

outfile: report  [
  required: "Y"
  default: "stdout"
  information: "Output report"
]

integer: wordsize  [
  required: "Y"
  default: "20"
  minimum: "2"
  information: "Word size"
]
