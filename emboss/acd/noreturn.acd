application: noreturn [
  documentation: "Removes carriage return from ASCII files"
  groups: "Edit"
]

section: input [
  information: "Input section"
  type: "page"
]

  infile: infile  [
    parameter: "Y"
    standardtype: "ascii text"
    information: "Input file"
  ]

endsection: input

section: output [
  information: "Output section"
  type: "page"
]

  outfile: outfile  [
    parameter: "Y"
    standardtype: "ascii text"
  ]

endsection: output
