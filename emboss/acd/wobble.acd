appl: wobble [
  documentation: "Wobble base plot"
  groups: "Nucleic:Gene finding"
]

sequence: sequence  [
  parameter: "Y"
  type: "dna"
]

string: bases  [
  optional: "N"
  default: "GC"
  information: "Bases used"
]

integer: window  [
  optional: "Y"
  default: "30"
  minimum: "1"
  information: "Window size in codons"
]

xygraph: graph  [
  required: "Y"
  multiple: "6"
]

outfile: outf  [
  required: "Y"
]
