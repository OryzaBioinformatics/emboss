appl: whichdb [
  documentation: "Search all databases for an entry"
  groups: "Information"
]

string: entry  [
  parameter: "Y"
  information: "ID or Accession number"
]

bool: get  [
  required: "N"
  default: "N"
  information: "Retrieve sequences"
]

bool: showall  [
  required: "N"
  default: "N"
  information: "Show failed attempts"
]

outfile: outfile  [
  required: "@(!$(get))"
  nullok: "$(get)"
]
