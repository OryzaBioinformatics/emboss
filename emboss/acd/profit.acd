application: profit [
  documentation: "Scan one or more sequences with a simple frequency matrix"
  groups: "Nucleic:Profiles, Protein:Profiles"
  relations: "/edam/topic/0000188 Sequence profiles"
  relations: "/edam/operation/0000300 Sequence-profile alignment"
]

section: input [
  information: "Input section"
  type: "page"
]

  infile: infile [
    parameter: "Y"
    information: "Profile or weight matrix file"
    knowntype: "profile or matrix"
    relations:  "/edam/data/0001354 Sequence profile"                               
  ]

  seqall: sequence [
    parameter: "Y"
    type: "any"
    relations: "/edam/data/0000848 Sequence" 
    relations: "/edam/data/0000849 Sequence record" 
    relations: "/edam/data/0002178 1 or more" 
  ]

endsection: input

section: output [
  information: "Output section"
  type: "page"
]

  outfile: outfile [
    parameter: "Y"
    knowntype: "profit output"
    relations:  "/edam/data/0000869 Sequence-profile alignment"                   
  ]

endsection: output
