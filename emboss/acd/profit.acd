application: profit [
  documentation: "Scan a sequence or database with a matrix
                  or profile"
  groups: "Nucleic:Profiles, Protein:Profiles"
]

section: input [
  information: "Input section"
  type: "page"
]

  infile: infile  [
    parameter: "Y"
    information: "Profile or matrix file"
    knowntype: "profile file;matrix file"
  ]

  seqall: sequence  [
    parameter: "Y"
    type: "any"
  ]

endsection: input

section: output [
  information: "Output section"
  type: "page"
]

  outfile: outfile  [
    parameter: "Y"
    knowntype: "profit output"
  ]

endsection: output
