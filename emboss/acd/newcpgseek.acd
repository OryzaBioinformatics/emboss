appl: newcpgseek [
  documentation: "Reports CpG rich regions"
  groups: "Nucleic:CpG Islands"
]

seqall: sequence  [
  parameter: "Y"
  type: "DNA"
]

integer: score  [
  required: "Y"
  minimum: "1"
  maximum: "200"
  default: "17"
  information: "CpG score"
]

outfile: outfile  [
  parameter: "Y"
]
