appl: stssearch [
  documentation: "Searches a DNA database for matches with a set of STS primers"
  groups: "Nucleic:Primers"
]

section: input [ info: "input Section" type: page ]
seqall: sequences  [
  parameter: "Y"
  type: "dna"
]

infile: primers  [
  parameter: "Y"
  prompt: "Primer file"
]

endsection: input

section: output [ info: "output Section" type: page ]
outfile: out  [
  parameter: "Y"
]
endsection: output

