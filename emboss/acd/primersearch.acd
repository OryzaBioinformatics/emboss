appl: primersearch [
  documentation: "Searches DNA sequences for matches with primer pairs"
  groups: "Nucleic:Primers"
]

seqall: sequences  [
  parameter: "Y"
  type: "dna"
]

infile: primers  [
  parameter: "Y"
  prompt: "Primer file"
]

outfile: out  [
  parameter: "Y"
]

integer: mismatchpercent  [
  parameter: "Y"
  default: "0"
  prompt: "Allowed percent mismatch"
]
