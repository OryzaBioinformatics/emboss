appl: findkm [
  documentation: "Find Km and Vmax for an enzyme reaction by a Hanes/Woolf plot"
  groups: "Enzyme Kinetics"
]

infile: infile  [
  parameter: "Y"
  prompt: "Enter name of file containing data"
]

outfile: outfile  [
  parameter: "Y"
]

bool: plot  [
  required: "N"
  default: "Y"
  information: "S/V vs S"
]

xygraph: graphlb  [
  required: "Y"
  multiple: "2"
]
