appl: stamps
[
        doc: "Generate alignments for SCOP families in EMBL-format SCOP file"
	groups: "PROTEIN: structure"
]

infile: scopf 	
[ 
	param: Y
	prompt: "Name of scop file for input (embl-like format)"
	def: "Escop.dat"
]

string: path
[
	param: Y
	prompt: "Location of alignment files for output"
	def: "./"
]

