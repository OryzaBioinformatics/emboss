appl: iep [
  documentation: "Calculates the isoelectric point of a protein"
  groups: "Protein:Composition"
]

seqall: sequencea  [
  parameter: "Y"
  type: "pureprotein"
]

bool: plot  [
  required: "N"
  default: "N"
  information: "Plot charge vs pH"
]

bool: report  [
  required: "N"
  default: "Y"
  information: "Write results to a file"
]

float: step  [
  required: "N"
  information: "pH step value"
  minimum: ".01"
  maximum: "1."
  default: ".5"
]

integer: amino  [
  required: "N"
  information: "Number of N-termini"
  default: "1"
]

bool: termini  [
  required: "N"
  default: "Y"
  information: "Include charge at N and C terminus"
]

xygraph: graph  [
  required: "$(plot)"
]

outfile: outfile  [
  required: "$(report)"
]
