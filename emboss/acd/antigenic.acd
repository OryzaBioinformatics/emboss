appl: antigenic [
  documentation: "Finds antigenic sites in proteins"
  groups: "Protein:Motifs"
]

seqall: sequence  [
  parameter: "Y"
  type: "PureProtein"
]

integer: minlen  [
  required: "Y"
  minimum: "1"
  maximum: "50"
  default: "6"
  information: "Minimum length"
]

report: outfile  [
  parameter: "Y"
  rformat: "motif"
  multiple: "Y"
  taglist: "int:pos=Max_score_pos"
]
