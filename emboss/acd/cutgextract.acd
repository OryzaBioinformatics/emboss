application: cutgextract [
  documentation: "Extract data from CUTG"
  groups: "Utils:Database creation"
]

section: input [
  information: "Input section"
  type: "page"
]

  dirlist: directory  [
    parameter: "Y"
    information: "CUTG directory"
  ]

endsection: input

section: advanced [
  information: "Advanced section"
  type: "page"
]

  string: wildspec  [
    default: "*.codon"
    information: "Type of codon file"
  ]

endsection: advanced
