appl: equicktandem [
  documentation: "Finds tandem repeats"
  groups: "Nucleic:Repeats"
]

sequence: sequence  [
  parameter: "Y"
  type: "dna"
]

outfile: outfile  [
  parameter: "Y"
  extension: "qtan"
]

integer: maxrepeat  [
  required: "Y"
  default: "600"
  information: "Maximum repeat size"
]

integer: threshold  [
  required: "Y"
  default: "20"
  information: "Threshold score"
]
