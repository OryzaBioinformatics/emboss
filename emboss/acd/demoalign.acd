appl: demoalign [
  documentation: "Reads a sequence set, writes an alignment file"
  groups: "Test"
]

seqset: sequence  [
  parameter: "Y"
  features: "Y"
]

align: outfile [
  parameter: "Y"
  aformat: "SRS"
]

matrixf: floatmatrix [
  optional: "Y"
  help: "Matrix file"
  protein: "$(sequence.protein)"
]

matrix: intmatrix [
  optional: "Y"
  help: "Matrix file"
  protein: "$(sequence.protein)"
]

bool: dofloat [
  prompt: "Use floating point matrix"
]
