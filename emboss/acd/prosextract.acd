application: prosextract [
  documentation: "Builds the PROSITE motif database for
                  patmatmotifs to search"
  groups: "Utils:Database creation"
  gui: "yes"
  batch: "yes"
  cpu: "medium"
]

section: input [
  information: "Input section"
  type: "page"
]

  string: infdat  [
    parameter: "Y"
    information: "Name of prosite directory"
  ]

endsection: input
