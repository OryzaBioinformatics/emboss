appl: wordcount [
  documentation: "Counts words of a specified size in a DNA sequence"
  groups: "Nucleic:Composition"
]

sequence: sequence  [
  parameter: "Y"
  type: "dna"
]

integer: wordsize  [
  required: "Y"
  default: "4"
  minimum: "2"
  information: "Word size"
]

outfile: outfile  [
  required: "Y"
]
