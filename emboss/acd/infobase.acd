application: infobase
[
  documentation: "Return information on a given nucleotide base"
  groups: "Information"
  gui: "yes"
  batch: "yes"
  cpu: "low"
  relations: "EDAM: Sequence description"
]





section: input 
[
  information: "Input section"
  type: "page"
]

  string: code
  [
    parameter: "Y"
    information: "Nucleotide base code"
    help: "Enter IUPAC nucleotide base codes, e.g. A, C, G, T, R, Y etc."
    knowntype: "nucleotide codes"
    default: "ABCDGHKMNRSTUVWXY"
    relations:  "EDAM: Nucleotide code string"                         
  ]

endsection: input





section: required 
[
  information: "Required section"
  type: "page"
]
endsection: required





section: additional
[
  information: "Additional section"
  type: "page"
]
endsection: additional





section: advanced
[
  information: "Advanced section"
  type: "page"
]
endsection: advanced





section: output 
[
  information: "Output section"
  type: "page"
]

  outfile: outfile
  [
    parameter: "Y"
    knowntype: "infobase output"
    relations:  "EDAM:Nucleotide base information report"                  
  ]
  

endsection: output
